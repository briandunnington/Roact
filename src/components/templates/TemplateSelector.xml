<?xml version="1.0" encoding="utf-8" ?>
<component name="TemplateSelector" extends="Group">
    <script type="text/brightscript" uri="pkg:/components/templates/TemplateSelector.brs"/>

    <interface>
        <field id="itemContent" type="node"/>
        <field id="width" type="float"/>
        <field id="height" type="float"/>
        <field id="focusPercent" type="float"/>
        <field id="rowFocusPercent" type="float"/>
        <field id="rowIndex" type="integer" alwaysNotify="true"/>
        <field id="rowHasFocus" type="boolean"/>
        <field id="rowListHasFocus" type="boolean"/>
        <field id="index" type="integer"/>
    </interface>

    <children>
        <VideoItem id="videoItem" visible="false"/>
        <BannerItem id="bannerItem" visible="false"/>
        <ButtonsItem id="buttonsItem" visible="false"/>
    </children>
</component>
